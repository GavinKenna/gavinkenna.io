{
  "name": "gavinkenna.io",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "current-git-commit": "GIT_COMMIT=`git rev-parse --short HEAD`",
    "dev": "npm run current-git-commit nuxt",
    "build": "NODE_OPTIONS=--max_old_space_size=8192 npm run current-git-commit nuxt build",
    "start": "NODE_OPTIONS=--max_old_space_size=8192 npm run current-git-commit nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint": "npm run lint:js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/vue-fontawesome": "^2.0.2",
    "@nuxt/content": "^1.9.0",
    "bootstrap": "^4.5.2",
    "bootstrap-vue": "^2.17.3",
    "core-js": "^3.6.5",
    "nuxt": "^2.14.6"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.15.2",
    "@fortawesome/free-brands-svg-icons": "^5.15.2",
    "@fortawesome/free-regular-svg-icons": "^5.15.2",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@nuxtjs/eslint-config": "^3.1.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@nuxtjs/google-analytics": "^2.4.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.10.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "git-describe": "^4.0.4",
    "nuxt-fontawesome": "^0.4.0",
    "reading-time": "^1.2.1"
  }
}
